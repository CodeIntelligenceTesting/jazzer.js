{
	"compilerOptions": {
		"target": "ES2022",
		"module": "NodeNext",
		"baseUrl": "./",
		"allowJs": true,
		"checkJs": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"composite": true,
		"alwaysStrict": true,
		"sourceMap": true
	}
}
