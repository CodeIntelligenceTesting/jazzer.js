{
	"name": "fuzzyeagle",
	"version": "0.0.0",
	"description": "Coverage-guided, in-process fuzzing for the Node.js",
	"homepage": "https://github.com/CodeIntelligenceTesting/fuzzyEagle#readme",
	"keywords": [
		"coverage-guided",
		"fuzzing"
	],
	"author": "",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/CodeIntelligenceTesting/fuzzyEagle/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeIntelligenceTesting/fuzzyEagle.git"
	},
	"main": "index.js",
	"scripts": {
		"compile": "tsc -p .",
		"build": "tsc",
		"test": "jest",
		"test:watch": "jest --watch",
		"lint": "eslint .",
		"lint:fix": "npm run lint -- --fix",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"format": "npm run prettier:fix && npm run lint:fix",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"istanbul-lib-hook": "^3.0.0",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@types/babel__core": "^7.1.19",
		"@types/istanbul-lib-hook": "^2.0.1",
		"@types/jest": "^28.1.4",
		"@types/node": "^18.0.0",
		"@types/yargs": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-jest": "^26.5.3",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1",
		"ts-jest": "^28.0.5",
		"typescript": "^4.7.4"
	}
}
