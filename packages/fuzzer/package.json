{
	"name": "@jazzer.js/fuzzer",
	"version": "0.0.0",
	"private": true,
	"description": "A libfuzzer-based fuzzer for Node",
	"homepage": "https://github.com/CodeIntelligenceTesting/fuzzyEagle#readme",
	"author": "Code Intelligence",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/CodeIntelligenceTesting/fuzzyEagle/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeIntelligenceTesting/fuzzyEagle.git",
		"directory": "packages/fuzzer"
	},
	"main": "dist/fuzzer.js",
	"types": "dist/fuzzer.d.ts",
	"scripts": {
		"prebuild": "prebuild --runtime napi --backend cmake-js --all --strip --verbose",
		"upload": "prebuild --runtime napi --backend cmake-js --all --strip --upload ${GITHUB_TOKEN}",
		"install": "prebuild-install --runtime napi || cmake-js rebuild --out build",
		"compile": "cmake-js compile --out build",
		"format": "clang-format -i *.cpp shared/*.cpp shared/*.h"
	},
	"binary": {
		"napi_versions": [
			3
		]
	},
	"dependencies": {
		"bindings": "^1.5.0",
		"cmake-js": "^6.3.2",
		"node-addon-api": "^5.0.0",
		"prebuild-install": "^7.1.1"
	},
	"devDependencies": {
		"@types/bindings": "^1.5.1",
		"@types/node": "^18.0.3",
		"clang-format": "^1.8.0",
		"prebuild": "^11.0.3"
	},
	"engines": {
		"node": ">= 14.0.0",
		"npm": ">= 7.0.0"
	}
}
